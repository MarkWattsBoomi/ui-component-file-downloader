{
  "name": "ui-custom-component",
  "version": "1.0.0",
  "description": "Small webpack based boilerplate for getting started with Boomi Flow UI components",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p --config webpack.production.config.js",
    "build-no-source-map": "webpack -p --config webpack.production.config.js --devtool none",
    "upload": "node upload.js",
    "test-unit": "jest --watch",
    "test-integration": "testcafe-live chrome ./src/__tests__/integration",
    "test-integration-remote": "testcafe-live remote ./src/__tests__/integration --qr-code",
    "update": "node update.js"
  },
  "author": "Dell Boomi",
  "license": "MIT",
  "dependencies": {
    "dedent": "^0.7.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.4",
    "@types/loglevel": "^1.5.3",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "5.2.0",
    "clipboardy": "^1.2.3",
    "colors": "^1.3.1",
    "css-loader": "0.28.11",
    "download": "^7.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fs-extra": "^7.0.1",
    "glob-promise": "^3.4.0",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "mime-types": "^2.1.19",
    "mini-css-extract-plugin": "^0.5.0",
    "react-test-renderer": "^16.4.1",
    "recursive-copy": "^2.0.9",
    "request-promise-native": "^1.0.5",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.21.0",
    "testcafe": "^0.20.4",
    "testcafe-browser-provider-browserstack": "^1.5.0",
    "testcafe-live": "^0.1.3",
    "three-way-merger": "^0.5.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.10.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "write-file-webpack-plugin": "4.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.tsx",
      "!src/index.tsx",
      "!**/node_modules/**",
      "!src/__test__/**/*.*"
    ],
    "setupFiles": [
      "./mocks/manywho.js"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/__tests__/unit.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/mocks/styles.js"
    }
  },
  "flow": {
    "tenantId": "d1b3e73f-3f2a-4159-8fbb-ceb11a84e28d",
    "id": "88b7b053-049b-402d-b614-597bed6a4b78",
    "versionId": "",
    "filenames": {
      "js": "FileDownloader.js",
      "css": "FileDownloader.css"
    }
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {}
}
